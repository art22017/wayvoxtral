{
  "api": {
    "key": "YOUR_GROQ_API_KEY",
    "model": "whisper-large-v3",
    "endpoint": "",
    "proxy": "http://127.0.0.1:2080"
  },
  "languages": {
    "auto_detect": true,
    "preferred": ["ru", "en"],
    "primary": "ru"
  },
  "hotkeys": {
    "toggle": "f9"
  },
  "audio": {
    "sample_rate": 16000,
    "channels": 1,
    "format": "wav",
    "chunk_size": 2048,
    "max_duration": 30
  },
  "ui": {
    "theme": "dark",
    "position": "top-center",
    "animation_duration_ms": 200
  },
  "behavior": {
    "auto_paste": true,
    "copy_to_clipboard": true,
    "show_notification": false
  }
}
